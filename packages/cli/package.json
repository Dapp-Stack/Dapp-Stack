{
  "name": "create-dapp-stack",
  "version": "0.4.0",
  "engines": {
    "node": ">=8.11"
  },
  "description": "DApp Stack CLI, generate a new project",
  "bin": {
    "create-dapp-stack": "bin/index.js"
  },
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist",
    "template",
    "bin"
  ],
  "scripts": {
    "watch": "tsc -w",
    "build": "tsc",
    "clean": "shx rm -rf lib",
