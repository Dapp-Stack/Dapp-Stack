{
  "name": "@solon/tslint-config",
  "version": "0.1.0",
  "engines": {
    "node": ">=8.11"
  },
  "description": "Lint rules related to Solon for TSLint",
  "main": "tslint.json",
  "scripts": {
    "watch_without_deps": "tsc -w",
    "build": "tsc && copyfiles -u 2 './lib/monorepo_scripts/**/*' ./scripts",
    "clean": "shx rm -rf lib scripts",
    "lint": "tslint --project .",
    "manual:postpublish": "yarn build; node ./scripts/postpublish.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/SolonProject/solon.git"
  },
  "keywords": [
    "tslint",
    "config",
    "Solon",
    "typescript",
    "ts"
  ],
  "author": {
    "name": "Anthony Laibe",
    "email": "anthony@laibe.cc"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/solonproject/solon/issues"
  },
  "homepage": "https://github.com/solonproject/solon/packages/tslint-config/README.md",
  "devDependencies": {
    "@solon/monorepo": "^0.1.0",
    "@types/lodash": "4.14.104",
    "copyfiles": "^2.0.0",
    "make-promises-safe": "^1.1.0",
    "shx": "^0.2.2",
    "typescript": "3.0.1"
  },
  "dependencies": {
    "lodash": "^4.17.5",
    "tslint": "5.11.0",
    "tslint-eslint-rules": "^4.1.1",
    "tslint-react": "^3.2.0",
    "tsutils": "2.22.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
